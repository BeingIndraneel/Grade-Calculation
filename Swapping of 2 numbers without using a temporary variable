// C code to swap using Arithmetic Operators
#include <stdio.h>
int main()
{
    int x = 11, y = 5;
 
    // Code to swap 'x' and 'y'
    x = x + y; // x now becomes 16
    y = x - y; // y becomes 11
    x = x - y; // x becomes 5
 
    printf("After Swapping: x = %d, y = %d", x, y);
 
    return 0;
}
